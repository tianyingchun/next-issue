{
  "name": "@semic/dev-server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "serve": "yarn ts-node-esm src/index.mts",
    "migration:generate-dev": "ts-node src/migration.ts -- generate dev",
    "migration:revert": "ts-node src/migration.ts -- revert",
    "migration:run": "ts-node src/migration.ts -- run",
    "test-unit": "jest --config jest.config.js --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "@vendure/common": "2.0.0",
    "@vendure/core": "2.0.0",
    "bullmq": "3.15.2",
    "mysql": "npm:mysql2@3.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.10",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript-operations": "2.5.6",
    "@types/ioredis": "^4.28.10",
    "@types/node": "18.11.9",
    "@vendure/testing": "2.0.0",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "graphql": "16.6.0",
    "terser": "5.15.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
