{
  "name": "next-issue",
  "version": "1.0.0",
  "description": "next bug report repo",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianyingchun/next-issue.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tianyingchun/next-issue/issues"
  },
  "homepage": "https://github.com/tianyingchun/next-issue#readme",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build-docker": "cross-env NEXT_BUILD_ENV_OUTPUT=standalone next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "yarn lint --fix",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "yarn build:twl",
    "build:twl": "twl -p \"**/globals.css\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "g:lint-staged-files": "lint-staged --allow-empty"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@hyperse-io/next-env": "^1.0.2",
    "@hyperse-io/tailwind-layer": "^1.0.3",
    "@mui/lab": "^5.0.0-alpha.162",
    "@mui/material": "^5.15.6",
    "@next/bundle-analyzer": "14.1.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/forms": "^0.5.7",
    "@urql/next": "1.1.0",
    "aos": "^2.3.4",
    "class-variance-authority": "0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.3",
    "keen-slider": "^6.8.6",
    "material-ui-popup-state": "5.0.10",
    "next": "14.1.0",
    "next-intl": "^3.5.3",
    "next-themes": "^0.2.1",
    "overlayscrollbars": "2.5.0",
    "overlayscrollbars-react": "0.5.4",
    "preline": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.49.3",
    "react-icons": "^5.0.1",
    "react-intersection-observer": "^9.5.3",
    "sharp": "^0.33.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "urql": "4.0.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "0.1.4",
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@playwright/test": "^1.41.1",
    "@types/aos": "3.0.7",
    "@types/node": "^20.11.10",
    "@types/prettier": "3.0.0",
    "@types/react": "^18.2.67",
    "@types/shell-quote": "1.7.5",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "autoprefixer": "^10.4.17",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "3.14.1",
    "husky": "9.0.7",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "shell-quote": "1.8.1",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=16.8"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "packageManager": "yarn@4.0.2"
}
