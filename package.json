{
  "name": "next-issue",
  "version": "1.0.0",
  "description": "next bug report repo",
  "homepage": "https://github.com/tianyingchun/next-issue#readme",
  "bugs": {
    "url": "https://github.com/tianyingchun/next-issue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tianyingchun/next-issue.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen --config ./codegen.cjs",
    "dev": "next dev --port 5001 --turbopack",
    "postinstall": "patch-package",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "next-lint": "next lint",
    "start": "next start --port 4001",
    "test": "vitest run",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.12.4",
    "@apollo/experimental-nextjs-app-support": "^0.11.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.3.0",
    "@next/bundle-analyzer": "15.1.2",
    "cookies-next": "^5.0.2",
    "gql.tada": "^1.8.10",
    "graphql": "^16.10.0",
    "next": "15.1.2",
    "next-intl": "^3.26.3",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@hyperse/eslint-config-hyperse": "^1.2.8",
    "@hyperse/graphql-codegen-enum": "^1.0.3",
    "@hyperse/next-env": "^1.1.3",
    "@svgr/webpack": "^8.1.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "autoprefixer": "^10.4.20",
    "code-inspector-plugin": "^0.18.3",
    "eslint": "^9.17.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": ">=16.8"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
